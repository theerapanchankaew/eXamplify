{
    "name": "functions",
    "description": "Cloud Functions for eXamplify",
    "scripts": {
        "build": "tsc",
        "serve": "npm run build && firebase emulators:start --only functions",
        "shell": "npm run build && firebase functions:shell",
        "start": "npm run shell",
        "deploy": "firebase deploy --only functions",
        "logs": "firebase functions:log"
    },
    "engines": {
        "node": "20"
    },
    "main": "lib/index.js",
    "dependencies": {
        "firebase-admin": "^12.0.0",
        "firebase-functions": "^4.5.0",
        "cors": "^2.8.5"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/cors": "^2.8.17",
        "typescript": "^5.0.0"
    },
    "private": true
}